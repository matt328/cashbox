{
  "name": "cashbox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "find src -name '*.html' -exec node_modules/.bin/html-beautify -qr {} + && prettier --write src/**/*.ts",
    "cz": "git-cz",
    "push:hook": "ng test --watch=false",
    "cy:open:ui": "ng e2e ui-test --watch",
    "cy:run:ui": "ng e2e ui-test",
    "storybook": "start-storybook -p 4201",
    "storybook:ci": "start-storybook -p 4201 --ci",
    "test:ui": "start-server-and-test storybook:ci http-get://localhost:4201 cy:run:ui",
    "test:ui:watch": "start-server-and-test storybook:ci http-get://localhost:4201 cy:open:ui"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run push:hook"
    }
  },
  "lint-staged": {
    "*.{ts,json,css,md}": [
      "prettier --write"
    ],
    "*.html": [
      "html-beautify -qr"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.2",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "^10.0.2",
    "@angular/compiler": "^10.0.2",
    "@angular/core": "^10.0.2",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^10.0.2",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "^10.0.2",
    "@angular/platform-browser-dynamic": "^10.0.2",
    "@angular/router": "^10.0.2",
    "@ncstate/sat-popover": "^6.0.0",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "firebase": "^7.13.1",
    "performant-array-to-tree": "^1.7.1",
    "ramda": "^0.27.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "^10.0.0",
    "@angular/compiler-cli": "^10.0.2",
    "@babel/core": "^7.10.4",
    "@nrwl/cypress": "^9.5.0",
    "@nrwl/workspace": "^9.5.0",
    "@storybook/angular": "^5.3.19",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.14",
    "@types/ramda": "^0.27.6",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.0-next.1",
    "commitizen": "^4.1.2",
    "cypress": "^4.9.0",
    "cz-conventional-changelog": "^3.2.0",
    "diff": "^4.0.2",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.2.5",
    "inquirer": "^7.2.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "js-beautify": "^1.11.0",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.0.1",
    "open": "^7.0.3",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0",
    "ts-mocks": "^3.0.1",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
